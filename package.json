{
  "name": "race4success",
  "version": "1.0.0",
  "description": "Race4Success - Empowering individuals to achieve their goals through structured racing and competition",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css && npm run build:js && npm run build:html",
    "build:css": "sass src/scss/main.scss public/css/style.css --style=compressed",
    "build:js": "webpack --mode=production",
    "build:html": "copy src\\views\\index.html public\\index.html",
    "vercel-build": "npm run build",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "sass src/scss/main.scss public/css/style.css --watch",
    "watch:js": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/js/**/*.js",
    "format": "prettier --write src/**/*.{js,css,scss,html}"
  },
  "keywords": [
    "racing",
    "competition",
    "goals",
    "success",
    "motivation",
    "tracking",
    "achievement"
  ],
  "author": "Race4Success Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loseyco/race4success.git"
  },
  "bugs": {
    "url": "https://github.com/loseyco/race4success/issues"
  },
  "homepage": "https://race4success.org"
}