{
  "name": "race4success",
  "version": "1.0.0",
  "description": "Race4Success - Empowering individuals to achieve their goals through structured racing and competition",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cross-env PORT=3001 nodemon server.js",
    "client": "cross-env PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "build:server": "node server.js",
    "vercel-build": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx,css,scss,html}"
  },
  "keywords": [
    "racing",
    "competition",
    "goals",
    "success",
    "motivation",
    "tracking",
    "achievement"
  ],
  "author": "Race4Success Team",
  "license": "MIT",
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "react-scripts": "5.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "homepage": "https://race4success.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/loseyco/race4success.git"
  },
  "bugs": {
    "url": "https://github.com/loseyco/race4success/issues"
  },
  "homepage": "https://race4success.org",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
